{
  "name": "super-heroes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.0",
    "redux": "4.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build": "react-scripts build && yarn build-sw && yarn clean-cra-sw",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true yarn test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.23",
    "cross-env": "7.0.3",
    "http-proxy-middleware": "2.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tnovau/super-heroes.git"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/sw-build.js",
      "!<rootDir>/src/sw-custom.js",
      "!<rootDir>/src/setupProxy.js"
    ]
  }
}
